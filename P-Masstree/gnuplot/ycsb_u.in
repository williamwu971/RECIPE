reset

outname="ycsb_u.pdf"

set terminal pdf dashed size 7,4
set out outname

set grid ytic

set ylabel "Throughput\n(ops/us)" font "Helvetica,32" offset -8.4, 0
set lmargin 20
set tmargin 10
set xlabel "YCSB uniform" font "Helvetica,32" offset 0,-1
set bmargin 6

set style fill solid border -1
set boxwidth 0.9
set xtic font "Helvetica,25" offset 0,-0.4
set ytic font "Helvetica,32"
set key font "Helvetica,30"
#set key outside left top vertical maxrows 3 at 4,800
set key maxrows 3 at 3.9,31
#set key out center top horizontal maxrows 2 maxcols 3
set linetype 1 lc rgb 'black'
set style histogram
#set style fill transparent solid 0.5 border

set yrange [0:20]
#set logscale y

filename="ycsb_u.txt"
plot  filename using ($2):xtic(1) with hist ti "dram-ralloc" linetype -1 lw 1 fs solid 0.5, \
      filename using ($3):xtic(1) with hist ti "dram-log" linetype 1 lw 1 fs solid 0.75, \
      filename using ($4):xtic(1) with hist ti "dram-obj" linetype 4 lw 1, \
      filename using ($5):xtic(1) with hist ti "ralloc-ralloc" linetype 5 lw 1, \
      filename using ($6):xtic(1) with hist ti "obj-obj" linetype 5 lw 1 fs solid 0.75, \
